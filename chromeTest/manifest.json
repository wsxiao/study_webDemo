{
  "manifest_version": 2,
  "name": "我的时钟",
  "version": "1.0",
  "description": "自动校验表单",
  "icons": {
    "16": "images/1.png",
    "48": "images/logo@2x.png",
    "128": "images/3.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/logo@2x.png",
      "38": "images/5.png"
    },
    "default_title": "我的时钟",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches":["*://mygiftcard.jd.com/giftcard/index.action"],
      "js":["js/jquery-3.1.1.min.js", "js/my_clock.js"]
    }
  ]
}
